<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:comp='urn:import:dk.dbc.dataio.gui.client.components'>
    <ui:with type="dk.dbc.dataio.gui.client.pages.item.show.Texts" field="txt"/>

    <g:HTMLPanel>

        <g:Button ui:field="backButton" text="{txt.label_Back}" />

        <li><comp:PromptedLabel ui:field="packaging" guiId="jobinfoPackagingPanel" prompt="{txt.promptJobInfo_Packaging}" /></li>
        <li><comp:PromptedLabel ui:field="format" guiId="jobinfoFormatPanel" prompt="{txt.promptJobInfo_Format}" /></li>
        <li><comp:PromptedLabel ui:field="charset" guiId="jobinfoCharsetPanel" prompt="{txt.promptJobInfo_Charset}" /></li>
        <li><comp:PromptedLabel ui:field="destination" guiId="jobinfoDestinationPanel" prompt="{txt.promptJobInfo_Destination}" /></li>
        <li><comp:PromptedLabel ui:field="mailForNotificationAboutVerification" guiId="jobinfoMailForNotificationAboutVerificationPanel" prompt="{txt.promptJobInfo_MailForNotificationAboutVerification}" /></li>
        <li><comp:PromptedLabel ui:field="mailForNotificationAboutProcessing" guiId="jobinfoMailForNotificationAboutProcessingPanel" prompt="{txt.promptJobInfo_MailForNotificationAboutProcessing}" /></li>
        <li><comp:PromptedLabel ui:field="resultMailInitials" guiId="jobinfoResultMailInitialsPanel" prompt="{txt.promptJobInfo_ResultMailInitials}" /></li>
        <li><comp:PromptedLabel ui:field="type" guiId="typePanel" prompt="{txt.promptJobInfo_Type}" /></li>
        <li><comp:PromptedLabel ui:field="jobCreationTime" guiId="jobCreationTimePanel" prompt="{txt.promptJobInfo_JobCreationTime}" /></li>
        <li><comp:PromptedLabel ui:field="jobCompletionTime" guiId="jobCompletionTimePanel" prompt="{txt.promptJobInfo_JobCompletionTime}" /></li>

    </g:HTMLPanel>

</ui:UiBinder>