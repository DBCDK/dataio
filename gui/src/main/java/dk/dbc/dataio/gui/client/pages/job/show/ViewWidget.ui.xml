<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:dio='urn:import:dk.dbc.dataio.gui.client.components'
             xmlns:c="urn:import:com.google.gwt.user.cellview.client">
    <ui:with type="dk.dbc.dataio.gui.client.pages.job.show.Texts" field="txt"/>

    <g:HTMLPanel>

        <table cellspacing='0' cellpadding='0' style='width:100%;'>
            <tr>
                <td id="jobs-show-table" valign='top'>
                    <c:CellTable pageSize='500' ui:field='jobsTable' />
                </td>
            </tr>
        </table>
        <li><g:Button ui:field="moreButton"><ui:text from="{txt.button_ShowMore}"/></g:Button></li>

    </g:HTMLPanel>

</ui:UiBinder>
