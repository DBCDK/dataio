
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="img" type="dk.dbc.dataio.gui.client.resources.Resources"/>
    <ui:with type="dk.dbc.dataio.gui.client.components.submitterfilter.Texts" field="txt"/>

    <g:HTMLPanel>

        <span>
            <Label><ui:text from="{txt.enabledFilter_Prompt}"/></Label>
            <g:RadioButton ui:field="enabledRadioButton" text="{txt.enabledFilter_Enabled}" name="enabledFilterRadioButtons"/>
            <g:RadioButton ui:field="disabledRadioButton" text="{txt.enabledFilter_Disabled}" name="enabledFilterRadioButtons"/>
        </span>

    </g:HTMLPanel>
</ui:UiBinder>
