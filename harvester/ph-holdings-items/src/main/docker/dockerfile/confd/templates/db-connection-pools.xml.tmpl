<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE resources PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Resource Definitions//EN" "http://glassfish.org/dtds/glassfish-resources_1_5.dtd">
<resources>
    <jdbc-connection-pool datasource-classname="org.postgresql.ds.PGSimpleDataSource" name="jdbc/phlog/pool">
        <property name="user" value="{{getv "/dbc/postgres/phlog/env/postgres/user"}}"></property>
        <property name="PortNumber" value="{{getv "/dbc/postgres/phlog/env/postgres/port"}}"></property>
        <property name="password" value="{{getv "/dbc/postgres/phlog/env/postgres/password"}}"></property>
        <property name="DriverClass" value="org.postgresql.Driver"></property>
        <property name="DatabaseName" value="{{getv "/dbc/postgres/phlog/env/postgres/db"}}"></property>
        <property name="serverName" value="{{getv "/dbc/postgres/phlog/env/postgres/host"}}"></property>
    </jdbc-connection-pool>
    <jdbc-resource pool-name="jdbc/phlog/pool" jndi-name="jdbc/phlog"></jdbc-resource>

    <jdbc-connection-pool datasource-classname="org.postgresql.ds.PGSimpleDataSource" name="jdbc/search/holdingsitems/pool">
        <property name="user" value="{{getv "/dbc/postgres/holdingsitems/env/postgres/user"}}"></property>
        <property name="PortNumber" value="{{getv "/dbc/postgres/holdingsitems/env/postgres/port"}}"></property>
        <property name="password" value="{{getv "/dbc/postgres/holdingsitems/env/postgres/password"}}"></property>
        <property name="DriverClass" value="org.postgresql.Driver"></property>
        <property name="DatabaseName" value="{{getv "/dbc/postgres/holdingsitems/env/postgres/db"}}"></property>
        <property name="serverName" value="{{getv "/dbc/postgres/holdingsitems/env/postgres/server"}}"></property>
    </jdbc-connection-pool>
    <jdbc-resource pool-name="jdbc/search/holdingsitems/pool" jndi-name="jdbc/search/holdingsitems"></jdbc-resource>
</resources>
