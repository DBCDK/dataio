
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="img" type="dk.dbc.dataio.gui.client.resources.Resources"/>
    <ui:with type="dk.dbc.dataio.gui.client.components.jobfilter.Texts" field="txt"/>

    <g:HTMLPanel>

        <span>
            <Label><ui:text from="{txt.errorFilter_FailedIn}"/></Label>
            <g:CheckBox ui:field="processingCheckBox" text="{txt.errorFilter_Processing}" checked="true"/>
            <g:CheckBox ui:field="deliveringCheckBox" text="{txt.errorFilter_Delivering}" checked="true"/>
            <g:CheckBox ui:field="jobCreationCheckBox" text="{txt.errorFilter_JobCreation}" checked="true"/>
       </span>

    </g:HTMLPanel>
</ui:UiBinder>
