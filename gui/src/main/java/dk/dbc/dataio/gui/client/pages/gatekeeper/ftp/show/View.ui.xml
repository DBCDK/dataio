
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c="urn:import:com.google.gwt.user.cellview.client">
    <ui:with type="dk.dbc.dataio.gui.client.pages.sink.show.Texts" field="txt"/>

    <g:HTMLPanel>

        <g:HorizontalPanel horizontalAlignment="left">
            <g:Button ui:field="refreshButton" text="{txt.button_Refresh}" />
        </g:HorizontalPanel>
        <table cellspacing='0' cellpadding='0' style='width:100%;'>
            <tr>
                <td id="ftp-show-table" valign='top'>
                    <c:CellTable pageSize='500' ui:field='ftpShowTable' />
                </td>
            </tr>
        </table>

    </g:HTMLPanel>

</ui:UiBinder>
