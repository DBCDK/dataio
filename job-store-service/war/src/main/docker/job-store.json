{
  "appName": "JobStore",
  "jdbc": {
    "jdbc/dataio/jobstore": {
      "url": "${JOBSTORE_DB_URL}",
      "maxSize": 200
    }
  },
  "mail": {
    "mail/dataio/jobstore/notifications": {
      "mailhost": "${MAIL_HOST}",
      "from": "${MAIL_FROM}",
      "user": "${MAIL_USER}"
    }
  },
  "hazelcast": {
    "clusterName": "${HZ_CLUSTER_NAME}",
    "imports": ["file:////opt/payara6/deployments/hz-map.xml"]
  }
}
