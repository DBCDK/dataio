<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c="urn:import:com.google.gwt.user.cellview.client"
             xmlns:fc="urn:import:dk.dbc.dataio.gui.client.pages.flowcomponent.show.jsmodulespopup">
    <ui:with type="dk.dbc.dataio.gui.client.pages.flowcomponent.show.Texts" field="txt"/>

    <ui:style>
    </ui:style>

    <g:HTMLPanel>

        <g:Button ui:field="createButton" text="{txt.button_Create}" />

        <c:CellTable pageSize='500' ui:field='flowComponentsTable' />

        <fc:PopupDoubleList ui:field="jsModulesPopup" dialogTitle="{txt.header_JSModulesListPopup}" okButtonText="{txt.button_Ok}"/>

    </g:HTMLPanel>

</ui:UiBinder>