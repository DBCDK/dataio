<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="txt" type="dk.dbc.dataio.gui.client.pages.navigation.Texts"/>
    <ui:with field="res" type="dk.dbc.dataio.gui.client.resources.Resources"/>

    <g:HTMLPanel styleName="{res.css.navigationPanelClass}">

        <g:Image resource="{res.dbcLogo}" styleName="{res.css.navigationPanelImage}"/>

        <g:Tree ui:field="menu">
            <g:TreeItem text="{txt.menu_NewJobs}" ui:field="newJobs"/>
            <g:TreeItem text="{txt.menu_Submitters}" ui:field="submitters">
                <g:TreeItem text="{txt.menu_SubmitterCreation}" ui:field="submitterCreation"/>
            </g:TreeItem>
            <g:TreeItem text="{txt.menu_Flows}" ui:field="flows">
                <g:TreeItem text="{txt.menu_FlowCreation}" ui:field="flowCreation"/>
                <g:TreeItem text="{txt.menu_FlowComponentCreation}" ui:field="flowComponentCreation"/>
                <g:TreeItem text="{txt.menu_FlowComponents}" ui:field="flowComponents"/>
                <g:TreeItem text="{txt.menu_FlowBinderCreation}" ui:field="flowBinderCreation"/>
                <g:TreeItem text="{txt.menu_FlowBinders}" ui:field="flowBinders"/>
            </g:TreeItem>
            <g:TreeItem text="{txt.menu_Sinks}" ui:field="sinks">
                <g:TreeItem text="{txt.menu_SinkCreation}" ui:field="sinkCreation"/>
            </g:TreeItem>
        </g:Tree>

    </g:HTMLPanel>

</ui:UiBinder>