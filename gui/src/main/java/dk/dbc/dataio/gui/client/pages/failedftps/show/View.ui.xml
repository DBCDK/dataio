<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:popup="urn:import:dk.dbc.dataio.gui.client.components.popup"
             xmlns:io='urn:import:dk.dbc.dataio.gui.client.pages.failedftps.show'>

    <ui:with type="dk.dbc.dataio.gui.client.pages.failedftps.show.Texts" field="txt"/>

    <g:HTMLPanel>

        <io:FailedFtpsTable pageSize='500' ui:field='failedFtpsTable' />

        <popup:PopupValueBox ui:field="editTransFilePopup" okButtonText="{txt.button_ResendTransfile}" cancelButtonText="{txt.button_Cancel}" dialogTitle="{txt.label_PageTitle}" autoHide="false"/>

    </g:HTMLPanel>

</ui:UiBinder>
