<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/application_7.xsd" version="7">
  <description>DataIO rawrepo harvester</description>
  <display-name>dataio-harvester-rawrepo-ear</display-name>
  <module>
    <ejb>dataio-commons-utils-binary-file-store-${project.version}.jar</ejb>
  </module>
  <module>
    <ejb>dataio-commons-utils-file-store-service-connector-${project.version}.jar</ejb>
  </module>
  <module>
    <ejb>dataio-commons-utils-job-store-service-connector-${project.version}.jar</ejb>
  </module>
  <module>
    <ejb>dataio-harvester-rawrepo-ejb-${project.version}.jar</ejb>
  </module>
  <library-directory>lib</library-directory>

  <env-entry>
    <env-entry-name>java:app/env/dataio/bfs/jndi/path</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>path/dataio/harvester/tmp</env-entry-value>
  </env-entry>
</application>
