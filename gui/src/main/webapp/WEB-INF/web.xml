<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>FlowStoreProxyServlet</servlet-name>
    <servlet-class>dk.dbc.dataio.gui.server.FlowStoreProxyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmbeddedEngine</servlet-name>
    <servlet-class>dk.dbc.dataio.gui.server.EmbeddedEngineServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FlowStoreProxyServlet</servlet-name>
    <url-pattern>/dk.dbc.dataio.gui.Main/FlowStoreProxy</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmbeddedEngine</servlet-name>
    <url-pattern>/dk.dbc.dataio.gui.Main/EmbeddedEngine</url-pattern>
  </servlet-mapping>
  <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>gui.html</welcome-file>
    </welcome-file-list>
</web-app>
