             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:popup="urn:import:dk.dbc.dataio.gui.client.components.popup"
             xmlns:io='urn:import:dk.dbc.dataio.gui.client.pages.failedftps.show'>

