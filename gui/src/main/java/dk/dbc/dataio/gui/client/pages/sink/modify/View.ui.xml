<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:dio='urn:import:dk.dbc.dataio.gui.client.components'>

    <ui:with type="dk.dbc.dataio.gui.client.pages.sink.modify.Texts" field="txt"/>

    <g:HTMLPanel>

        <li><dio:TextEntry ui:field="name" guiId="sinknamepanel" prompt="{txt.label_SinkName}" maxLength="160" toolTip=""/></li>
        <li><dio:TextEntry ui:field="resource" guiId="sinkresourcepanel" prompt="{txt.label_ResourceName}" maxLength="160" toolTip=""/></li>
        <li><g:Button ui:field="saveButton"><ui:text from="{txt.button_Save}"/></g:Button></li>
        <li><g:Label ui:field="status"></g:Label></li>

    </g:HTMLPanel>

</ui:UiBinder>