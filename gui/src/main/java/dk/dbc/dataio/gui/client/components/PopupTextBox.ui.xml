<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>

        <g:DialogBox ui:field="dialogBox" animationEnabled="true" glassEnabled="true" modal="false">
            <g:VerticalPanel>
                <g:TextBox ui:field="text"/>
                <g:FlowPanel>
                    <g:Button ui:field="okButton"></g:Button>
                    <g:Button ui:field="cancelButton"></g:Button>
                </g:FlowPanel>
            </g:VerticalPanel>
        </g:DialogBox>

    </g:HTMLPanel>
</ui:UiBinder>